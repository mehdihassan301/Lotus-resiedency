{
  "redirects": [
    {
      "source": "http://beachviewstays.pk/:path*",
      "destination": "https://beachviewstays.pk/:path*",
      "permanent": true
    },
    {
      "source": "https://www.beachviewstays.pk/:path*",
      "destination": "https://beachviewstays.pk/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
